{
  "name": "formidable-frontend",
  "version": "0.0.0",
  "scripts": {
    "start": "npm run dev",
    "local": "ng serve --proxy-config proxy/proxy.local.conf.json",
    "dev": "ng serve --proxy-config proxy/proxy.dev.conf.json",
    "live": "ng serve --proxy-config proxy/proxy.live.conf.json",
    "build": "ng build",
    "lint": "ng lint",
    "test": "ng test",
    "dev:ssr": "ng run formidable-frontend:serve-ssr",
    "serve:ssr": "node dist/formidable-frontend/server/main.js",
    "build:ssr": "ng build --prod && ng run formidable-frontend:server:production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.0",
    "@angular/cdk": "^11.2.0",
    "@angular/common": "~11.2.0",
    "@angular/compiler": "~11.2.0",
    "@angular/core": "^11.2.0",
    "@angular/forms": "~11.2.0",
    "@angular/material": "^11.2.0",
    "@angular/material-moment-adapter": "^11.2.0",
    "@angular/platform-browser": "~11.2.0",
    "@angular/platform-browser-dynamic": "~11.2.0",
    "@angular/platform-server": "^11.2.0",
    "@angular/router": "~11.2.0",
    "@nguniversal/express-engine": "^11.2.0",
    "bootstrap": "^5.0.0-beta2",
    "express": "^4.17.1",
    "moment": "^2.29.1",
    "rxjs": "~6.6.6",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.2",
    "@angular-eslint/builder": "1.2.0",
    "@angular-eslint/eslint-plugin": "1.2.0",
    "@angular-eslint/eslint-plugin-template": "1.2.0",
    "@angular-eslint/schematics": "1.2.0",
    "@angular-eslint/template-parser": "1.2.0",
    "@angular/cli": "^11.2.2",
    "@angular/compiler-cli": "~11.2.0",
    "@nguniversal/builders": "^11.2.0",
    "@types/express": "^4.17.11",
    "@types/jasmine": "~3.6.4",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "4.20.0",
    "@typescript-eslint/parser": "4.15.2",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "32.2.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "^3.3.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^6.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "stylelint": "^13.11.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "ts-node": "^9.1.1",
    "typescript": "~4.1.5"
  }
}
