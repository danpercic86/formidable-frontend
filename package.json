{
  "name": "formidable-frontend",
  "version": "0.0.0",
  "scripts": {
    "start": "npm run dev",
    "local": "ng serve --proxy-config proxy/proxy.local.conf.json",
    "dev": "ng serve --proxy-config proxy/proxy.dev.conf.json",
    "live": "ng serve --proxy-config proxy/proxy.live.conf.json",
    "build": "ng build",
    "lint": "ng lint",
    "test": "ng test",
    "dev:ssr": "ng run formidable-frontend:serve-ssr",
    "serve:ssr": "node dist/formidable-frontend/server/main.js",
    "build:ssr": "ng build --prod && ng run formidable-frontend:server:production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.13",
    "@angular/cdk": "^11.2.9",
    "@angular/common": "~11.2.13",
    "@angular/compiler": "~11.2.13",
    "@angular/core": "^11.2.13",
    "@angular/forms": "~11.2.13",
    "@angular/material": "^11.2.9",
    "@angular/material-moment-adapter": "^11.2.9",
    "@angular/platform-browser": "~11.2.13",
    "@angular/platform-browser-dynamic": "~11.2.13",
    "@angular/platform-server": "^11.2.13",
    "@angular/router": "~11.2.13",
    "@nguniversal/common": "^11.2.1",
    "@nguniversal/express-engine": "^11.2.1",
    "express": "^4.17.1",
    "moment": "^2.29.1",
    "rxjs": "~6.6.7",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^4.2.0",
    "@angular-devkit/build-angular": "~0.1102.12",
    "@angular-eslint/eslint-plugin": "^4.2.0",
    "@angular-eslint/eslint-plugin-template": "^4.2.0",
    "@angular-eslint/schematics": "4.2.0",
    "@angular-eslint/template-parser": "^4.2.0",
    "@angular/cli": "^11.2.12",
    "@angular/compiler-cli": "~11.2.13",
    "@nguniversal/builders": "^11.2.1",
    "@types/express": "^4.17.11",
    "@types/jasmine": "~3.7.1",
    "@types/node": "^15.0.2",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "34.0.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "^3.4.0",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.6.0",
    "prettier": "^2.3.0",
    "protractor": "~7.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "ts-node": "^9.1.1",
    "typescript": "~4.1.5"
  }
}
